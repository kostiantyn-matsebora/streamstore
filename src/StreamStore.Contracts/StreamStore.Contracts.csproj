<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <IsPackable>True</IsPackable>
    <Nullable>enable</Nullable>
    <SignAssembly>False</SignAssembly>
    <Title>StreamStore.Contracts</Title>
    <Description>Abstractions for creating specific database backend implementation used by StreamStore asynchronous event sourcing library.</Description>
    <PackageProjectUrl>https://github.com/kostiantyn-matsebora/streamstore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kostiantyn-matsebora/streamstore</RepositoryUrl>
    <Authors>kostiantyn-matsebora</Authors>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageId>StreamStore.Contracts</PackageId>
    <Version>0.11.0</Version>
    <RootNamespace>StreamStore</RootNamespace>
    <Copyright>since 2024 Â© Kostiantyn Matsebora. All rights reserved.</Copyright>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>event sourcing; streams; stream store; async</PackageTags>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="8.0.2" />
  </ItemGroup>
</Project>
